{% extends '@nucleus/partials/particle.html.twig' %}

{% block particle %}
<ul class="social-buttons">
	 {% for sm in particle.buttons %}
	 	<li class="social-button">
			<a href="{{ sm.url }}"
				class="{% if sm.class %}{{sm.class}}{% endif %}"
			>
				<i class="fa {{sm.icon}} center-align"></i>
			</a>
		</li>
	 {% endfor %}
</ul>
{% endblock %}
